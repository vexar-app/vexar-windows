{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Vexar",
  "version": "0.1.0",
  "identifier": "com.consolaktif.vexar",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Vexar",
        "width": 380,
        "height": 700,
        "resizable": false,
        "fullscreen": false,
        "center": true,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "externalBin": [
      "binaries/spoofdpi"
    ],
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {}
  }
}
